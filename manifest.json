{
  "manifest_version": 3,
  "name": "cookie-collector",
  "description": "collect cookie in specify website",
  "version": "1.0.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "cookies", "activeTab", "tabs", "webRequest"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "commands":{
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  }
}